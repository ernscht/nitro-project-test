{
	"$schema": "http://json-schema.org/schema#",
	"title": "<%= pattern.name %>",
	"description": "A <%= pattern.name %> <%= pattern.type %>",
	"type": "object",
	"properties": {<% if (modifier.name) { %>
		"modifier": {
			"description": "A pattern modifier",
			"enum": ["", "<%= modifier.css %>"]
		}<% } %>
	},
	"required": [<% if (modifier.name) { %>"modifier"<% } %>],
	"dependencies": {}
}
