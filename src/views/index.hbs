<div class="container">
	<div class="row">
		<div class="col-md-8 offset-md-2">
			<h1 style="font-size:2em;margin-top:1em;">{{pattern name='icon' icon='nitro'}}&nbsp;Nitro example page [{{theme.name}}]</h1>
			{{pattern name='example'}}
			{{pattern name='example' data='example-highlight'}}
			{{pattern name='example' data='example-template'}}
			{{#pattern name='box'}}
				<a href="example-patterns">more examples</a>
			{{/pattern}}
		</div>
	</div>
</div>

{{#if _nitro.production}}
	<div class="container">
		<div class="row">
			<div class="col-md-8 offset-md-2">
				<div class="proto-themelist">
					<h2>Available Themes:</h2>
					{{themelist current=theme.id}}
				</div>
			</div>
		</div>
	</div>
{{/if}}
